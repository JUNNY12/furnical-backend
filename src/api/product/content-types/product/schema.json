{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "biginteger"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "rating": {
      "type": "integer"
    },
    "purchased": {
      "type": "integer"
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    }
  }
}
